{
  "name": "swapi-challenge",
  "version": "1.0.0",
  "description": "An application that works with swapi API",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "serve": "nodemon --exec babel-node index.js",
    "pretest": "cross-env NODE_ENV=test && npm run migrations-down:test && npm run migrations-up:test",
    "test": "cross-env NODE_ENV=test nyc mocha --exit --require @babel/register --require @babel/polyfill --timeout 600000 server/./test/**/*.spec.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov --require babel-core/register  _mocha --exit test/*.js",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info",
    "migrations-down:test": "db-migrate down --config database.json -e test",
    "migrations-up:test": "db-migrate up --config database.json -e test",
    "migrations-down:dev": "db-migrate down --config database.json -e development",
    "migrations-up:dev": "db-migrate up --config database.json -e development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BukkyOmo/swapi-challenge.git"
  },
  "keywords": [
    "Third-party",
    "API",
    "node",
    "postgres"
  ],
  "author": "Bukola Omosefunmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BukkyOmo/swapi-challenge/issues"
  },
  "homepage": "https://github.com/BukkyOmo/swapi-challenge#readme",
  "dependencies": {
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "app-root-path": "^2.2.1",
    "axios": "^0.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "dotenv": "^8.2.0",
    "editorconfig": "^0.15.3",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "redis": "^2.8.0",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3",
    "validatorjs": "^3.17.1",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^11.7.0"
  }
}
